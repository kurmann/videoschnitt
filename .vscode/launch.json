{
  "version": "0.2.0",
  "configurations": [
      {
          "name": ".NET Core Launch (web)",
          "type": "coreclr",
          "request": "launch",
          "preLaunchTask": "build",
          "program": "${workspaceFolder}/src/Application/bin/Debug/net9.0/osx-arm64/Kurmann.Videoschnitt.Application.dll",
          "args": [],
          "cwd": "${workspaceFolder}/src/Application",
          "stopAtEntry": false,
          "serverReadyAction": {
              "action": "openExternally",
              "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
          },
          "env": {
              "ASPNETCORE_ENVIRONMENT": "Development",
              "ASPNETCORE_URLS": "http://localhost:5023"
          },
          "sourceFileMap": {
              "/Views": "${workspaceFolder}/Views"
          }
      },
      {
          "name": ".NET Core Launch (console)",
          "type": "coreclr",
          "request": "launch",
          "preLaunchTask": "build",
          "program": "${workspaceFolder}/src/ConsoleApp/bin/Debug/net9.0/osx-arm64/Kurmann.Videoschnitt.ConsoleApp.dll",
          "args": ["--workflow", "HealthCheck"],
          "cwd": "${workspaceFolder}/src/ConsoleApp",
          "stopAtEntry": false,
          "env": {
              "ASPNETCORE_ENVIRONMENT": "Development"
          },
          "console": "integratedTerminal"
      }
  ]
}