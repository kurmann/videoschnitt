{
  "version": "0.2.0",
  "configurations": [
      {
          "name": ".NET Core Launch (FinalCutPro)",
          "type": "coreclr",
          "request": "launch",
          "preLaunchTask": "build",
          "program": "${workspaceFolder}/src/ConsoleApp/bin/Debug/net9.0/osx-arm64/Kurmann.Videoschnitt.ConsoleApp.dll",
          "args": ["--workflow", "FinalCutPro", "--environment", "Development"],
          "cwd": "${workspaceFolder}/src/ConsoleApp",
          "stopAtEntry": false,
          "env": {
              "ASPNETCORE_ENVIRONMENT": "Development"
          },
          "console": "integratedTerminal"
      },
      {
        "name": ".NET Core Launch (InfuseMediaLibrary)",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/src/ConsoleApp/bin/Debug/net9.0/osx-arm64/Kurmann.Videoschnitt.ConsoleApp.dll",
        "args": ["--workflow", "InfuseMediaLibrary", "--environment", "Development"],
        "cwd": "${workspaceFolder}/src/ConsoleApp",
        "stopAtEntry": false,
        "env": {
            "ASPNETCORE_ENVIRONMENT": "Development"
        },
        "console": "integratedTerminal"
    }
  ]
}